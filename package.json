{
    "name": "extension-creator",
    "publisher": "spencerjames",
    "version": "0.0.5",
    "description": "Create and publish your own Visual Studio Code extensions",
    "displayName": "VSCode Extension Creator",
    "shortName": "Extension Creator",
    "homepage": "https://github.com/kokoabim/vscode-extension-template",
    "bugs": {
        "url": "https://github.com/kokoabim/vscode-extension-template/issues"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/kokoabim/vscode-extension-template"
    },
    "license": "SEE LICENSE IN LICENSE.md",
    "icon": "icon/extension-512.png",
    "engines": {
        "vscode": "^1.96.0"
    },
    "categories": [
        "Programming Languages",
        "Snippets",
        "Testing",
        "Education"
    ],
    "keywords": [
        "vscode",
        "extension",
        "template",
        "project",
        "create",
        "publish",
        "typescript",
        "vsce"
    ],
    "activationEvents": [],
    "main": "./dist/extension.js",
    "contributes": {
        "configuration": {
            "title": "Extension Creator",
            "properties": {
                "extension-creator.yourName": {
                    "type": "string",
                    "default": "World",
                    "markdownDescription": "What is _your_ name?"
                }
            }
        },
        "commands": [
            {
                "command": "extension-creator.open-new-extension-project-panel",
                "title": "Create Visual Studio Code Extension..."
            }
        ],
        "viewsWelcome": [
            {
                "view": "explorer",
                "contents": "Create a Visual Studio Code extension project.\n\n[Learn more](https://github.com/kokoabim/vscode-extension-template/blob/main/README.md) about creating, testing and publishing Visual Studio Code extensions.\n[Create VS Code Extension](command:extension-creator.create-new-extension-project)",
                "when": "workspaceFolderCount == 0"
            }
        ]
    },
    "scripts": {
        "compile": "tsc -p ./tsconfig.json",
        "install-compile": "./npmx.sh -bcy install",
        "lint": "eslint ./src",
        "pretest": "./npmx.sh -cly install",
        "test": "vscode-test",
        "vscode:prepublish": "./npmx.sh -y prepublish",
        "watch": "tsc -watch -p ./tsconfig.json"
    },
    "devDependencies": {
        "@types/node": "^22.x",
        "@typescript-eslint/eslint-plugin": "^8.26.1",
        "@typescript-eslint/parser": "^8.26.1",
        "@vscode/test-cli": "^0.0.10",
        "@vscode/test-electron": "^2.4.1",
        "eslint": "^9.23.0",
        "ts-loader": "^9.5.2",
        "ts-node": "^10.9.2",
        "typescript": "^5.8.2",
        "webpack": "^5.97.1",
        "webpack-cli": "^6.0.1"
    },
    "dependencies": {
        "@types/vscode": "^1.96.0",
        "@vscode-elements/elements": "^1.14.0",
        "@vscode/codicons": "^0.0.36",
        "extract-zip": "^2.0.1"
    }
}