{
    "compilerOptions": {
        "module": "Node16",
        "target": "ES2022",
        "lib": [
            "ES2022"
        ],
        "rootDir": "src",
        "outDir": "dist",

        "esModuleInterop": true,
        "forceConsistentCasingInFileNames": true,
        "noImplicitReturns": true,
        "sourceMap": true,
        "strict": true,
    }
}